//Calcolare le tabelline di tutti i numeri naturali compresi tra 2 e 10 e stampare una tabellina per riga
#include<stdio.h>
#include<math.h>
#include<stdlib.h>
int main()
{
 int tab;
 int num;		
 int i;		
 for (num=2;num<=10;num++){	
		tab=0;
		printf("La tabellina di %d e':",num);				
		for (i=0;i<10;i++){	
 		  tab+=num;				
		  printf("\t%d",tab);	
		}
		printf("\n");			
	}
 }
