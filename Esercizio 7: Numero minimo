#include<stdio.h>
#include<stdlib.h>
int main() {
	int n;
	int i;
	int num;
	int min;
	printf("Inserire quanti numeri si vuole mettere:");
	scanf("%d",&n);
	i=1;
	printf("Inserisci primo numero:");
	scanf("%d",&num);
	min=num;
	while(i<n) {
		printf("Inserisci numero:");
		scanf("%d",&num); 
			if(num<min) {
				min=num;
			}
			i++;
	}
	printf("Il numero minimo e':%d",min);
}
